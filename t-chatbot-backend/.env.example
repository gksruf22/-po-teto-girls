# .env.example
# 로컬 개발 및 배포 시 필요한 환경 변수 예시
# 실제 사용 시 .env 파일로 복사하고 값을 채워주세요

# Database Configuration (Supabase PostgreSQL)
DATABASE_URL=jdbc:postgresql://your-supabase-host:5432/postgres
DB_USERNAME=postgres
DB_PASSWORD=your-database-password

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-at-least-256-bits-long-please-change-this
JWT_EXPIRATION=86400000

# Google Gemini API
GOOGLE_API_KEY=your-google-gemini-api-key

# CORS Configuration
# 콤마로 구분하여 여러 도메인 지정 가능
CORS_ALLOWED_ORIGINS=https://your-frontend-domain.vercel.app,https://your-custom-domain.com

# Server Configuration (Railway/Render에서 자동 설정됨)
PORT=8080

# Spring Profile
SPRING_PROFILES_ACTIVE=prod
